{
  "manifest_version": 2,
  "name": "Center Image",
  "version": "2.1",
  "description": "Center display of directly-viewed images",
  "icons": {
    "48": "desoto-48.png",
    "32": "desoto-32.png"
  },
  "options_page": "options.html",
  "content_scripts": [
    {
      "js": [ "center-image.js" ],
      "matches": ["<all_urls>"],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "storage"
  ]
}
